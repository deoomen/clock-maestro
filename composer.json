{
    "name": "deoomen/clock-maestro",
    "description": "Single source of truth about time and date in your application",
    "type": "library",
    "keywords": ["time", "clock", "date", "manager", "SSOT"],
    "license": "MIT",
    "authors": [
        {
            "name": "deoomen",
            "email": "deoomen@pm.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClockMaestro\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClockMaestro\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
